<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_graphical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/nine"
        android:id="@+id/b2"
        android:layout_alignTop="@+id/b1"
        android:layout_centerHorizontal="true"
        android:background="?android:attr/colorBackground" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/three"
        android:id="@+id/b5"
        android:layout_alignTop="@+id/b4"
        android:layout_alignStart="@+id/b2"
        android:background="?android:attr/colorBackground" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/four"
        android:id="@+id/b6"
        android:background="?android:attr/colorBackground"
        android:layout_alignTop="@+id/b5"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/oneo"
        android:id="@+id/b1"
        android:background="?android:attr/colorBackground"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/eght"
        android:id="@+id/b8"
        android:background="?android:attr/colorBackground"
        android:layout_alignTop="@+id/b7"
        android:layout_alignStart="@+id/b5" />


    <Button
        android:text="Go"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/goo"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/b8" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/svn"
        android:id="@+id/b10"
        android:background="?android:attr/colorBackground"
        android:layout_marginTop="12dp"
        android:layout_below="@+id/b7"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/five"
        android:id="@+id/b7"
        android:background="?android:attr/colorBackground"
        android:layout_below="@+id/b4"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/two"
        android:layout_marginTop="11dp"
        android:id="@+id/b4"
        android:background="?android:attr/colorBackground"
        android:layout_below="@+id/b1"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/eleven"
        android:id="@+id/b11"
        android:background="?android:attr/colorBackground"
        android:layout_alignTop="@+id/b10"
        android:layout_alignStart="@+id/b8" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ten"
        android:id="@+id/b3"
        android:background="?android:attr/colorBackground"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/six"
        android:id="@+id/b9"
        android:background="?android:attr/colorBackground"
        android:layout_below="@+id/b6"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/twlve"
        android:id="@+id/b12"
        android:background="?android:attr/colorBackground"
        android:layout_alignTop="@+id/b11"
        android:layout_alignParentEnd="true" />
</RelativeLayout>
